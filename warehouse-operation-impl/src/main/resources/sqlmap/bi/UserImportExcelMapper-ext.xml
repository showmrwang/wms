<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">


<mapper namespace="com.baozun.scm.primservice.whoperation.dao.bi.UserImportExcelDao">
	<resultMap id="UserImportExcelResultExt" type="com.baozun.scm.primservice.whoperation.model.bi.UserImportExcel">
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="userImportExcelColumnsExt">
	    <![CDATA[
	        	id as id,
	        	import_code as importCode,
	        	file_name as fileName,
	        	error_file_name as errorFileName,
	        	user_id as userId,
	        	type as type,
	        	ou_id as ouId,
	        	create_time as createTime,
	        	is_success as isSuccess
	    ]]>
	</sql>
	
	<sql id="userImportExcelDynamicWhereExt">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用rapid中的Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>
	       
				<if test="id!=null">
					and id = #{id}
				</if>
	       
	       
		       <if test="importCode!=null and importCode!=''">
					and import_code = #{importCode}
				</if>
	       
	       
		       <if test="fileName!=null and fileName!=''">
					and file_name = #{fileName}
				</if>
	       
	       
		       <if test="errorFileName!=null and errorFileName!=''">
					and error_file_name = #{errorFileName}
				</if>
	       
				<if test="userId!=null">
					and user_id = #{userId}
				</if>
	       
	       
		       <if test="type!=null and type!=''">
					and type = #{type}
				</if>
	       
				<if test="ouId!=null">
					and ou_id = #{ouId}
				</if>
	       
				<if test="createTime!=null">
					and create_time = #{createTime}
				</if>
	       
				<if test="isSuccess!=null">
					and is_success = #{isSuccess}
				</if>
		</where>
	</sql>
	
	 <select id="findUserImportExcelByIdAndOuId" resultMap="UserImportExcelResultExt">
    	select <include refid="userImportExcelColumnsExt" />
	    from t_bi_user_import_excel 
		where 
		id = #{id} 
		and ou_id = #{ouid}

    </select>
	
</mapper>
