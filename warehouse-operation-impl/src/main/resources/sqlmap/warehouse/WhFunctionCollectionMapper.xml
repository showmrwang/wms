<mapper namespace="com.baozun.scm.primservice.whoperation.dao.warehouse.WhFunctionCollectionDao">
	<resultMap id="WhFunctionCollectionResult" type="com.baozun.scm.primservice.whoperation.model.warehouse.WhFunctionCollection">
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="whFunctionCollectionColumns">
	    <![CDATA[
	        	id as id,
	        	function_id as functionId,
	        	collection_pattern as collectionPattern,
	        	container_qty as containerQty,
	        	ou_id as ouId
	    ]]>
	</sql>
	
		<!-- 如果有自增字段 -->
		
			<!-- useGeneratedKeys="true" keyProperty="xxx" for sqlserver and mysql -->
			<insert id="insert" parameterType="com.baozun.scm.primservice.whoperation.model.warehouse.WhFunctionCollection" 
				
			 flushCache="true">
		    <![CDATA[
		        INSERT INTO
		        t_wh_function_collection (
		        id,
		        	function_id ,
		        	collection_pattern ,
		        	container_qty ,
		        	ou_id 
		        ) VALUES (
		        #{id,javaType=java.lang.Long,jdbcType=BIGINT} ,
		        	#{functionId,javaType=java.lang.Long,jdbcType=BIGINT} ,
		        	#{collectionPattern,javaType=java.lang.Integer,jdbcType=INTEGER} ,
		        	#{containerQty,javaType=java.lang.Integer,jdbcType=INTEGER} ,
		        	#{ouId,javaType=java.lang.Long,jdbcType=BIGINT} 
		        )
		    ]]>

		</insert>
		
		
		<update id="update" parameterType="com.baozun.scm.primservice.whoperation.model.warehouse.WhFunctionCollection">
	    <![CDATA[
	        UPDATE t_wh_function_collection SET
		        function_id = #{functionId,javaType=java.lang.Long,jdbcType=BIGINT} ,
		        collection_pattern = #{collectionPattern,javaType=java.lang.Integer,jdbcType=INTEGER} ,
		        container_qty = #{containerQty,javaType=java.lang.Integer,jdbcType=INTEGER} ,
		        ou_id = #{ouId,javaType=java.lang.Long,jdbcType=BIGINT} 
	        WHERE 
		        id = #{id} 
		        and ou_id = #{ouId,javaType=java.lang.Long,jdbcType=BIGINT}
	    ]]>
		</update>
	   <delete id="delete" parameterType="java.lang.Long">
	    <![CDATA[
	        delete from t_wh_function_collection where
		        id = #{id} 
	    ]]>
	    </delete> 
	    
	     <delete id="deleteByIds" parameterType="list">
	    
	        delete from t_wh_function_collection 
	        where id in
				<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
#{item}				</foreach>	
	  
	    </delete>  
	         
	    <select id="findById" parameterType="java.lang.Long" resultMap="WhFunctionCollectionResult" flushCache="false">
			select <include refid="whFunctionCollectionColumns" />
		    <![CDATA[
			    from t_wh_function_collection 
		        where 
		        id = #{id} 
		    ]]>
		</select>	
		<select id="getByIds" parameterType="list"  resultMap="WhFunctionCollectionResult" flushCache="false">
			select <include refid="whFunctionCollectionColumns" />
			    from t_wh_function_collection 
		        where id in
				<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
#{item}				</foreach>					
		</select>	
	<sql id="whFunctionCollectionDynamicWhere">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用rapid中的Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>
	       
				<if test="id!=null">
					and id = #{id}
				</if>
	       
				<if test="functionId!=null">
					and function_id = #{functionId}
				</if>
	       
				<if test="collectionPattern!=null">
					and collection_pattern = #{collectionPattern}
				</if>
	       
				<if test="containerQty!=null">
					and container_qty = #{containerQty}
				</if>
	       
				<if test="ouId!=null">
					and ou_id = #{ouId}
				</if>
		</where>
	</sql>
	

	<select id="getCount" resultType="long">
        select count(*) from t_wh_function_collection 
    </select>
    <select id="findListCountByQueryMap" resultType="long">
        select count(*) from t_wh_function_collection 
		<include refid="whFunctionCollectionDynamicWhere"/>    
    </select>
    
    <select id="findListCountByParam" resultType="long">
        select count(*) from t_wh_function_collection 
		<include refid="whFunctionCollectionDynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 offset,pageSize,lastRows 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="findListByQueryMapWithPage" resultMap="WhFunctionCollectionResult">
    	select <include refid="whFunctionCollectionColumns" />
	    from t_wh_function_collection 
		<include refid="whFunctionCollectionDynamicWhere"/>
		
    </select>
    
    <select id="findListByQueryMap" resultMap="WhFunctionCollectionResult">
    	select <include refid="whFunctionCollectionColumns" />
	    from t_wh_function_collection 
		<include refid="whFunctionCollectionDynamicWhere"/>
		
    </select>
    
    <select id="findListByParam" resultMap="WhFunctionCollectionResult">
    	select <include refid="whFunctionCollectionColumns" />
	    from t_wh_function_collection 
		<include refid="whFunctionCollectionDynamicWhere"/>
		
    </select>

	
</mapper>
