<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">


<mapper namespace="com.baozun.scm.primservice.whoperation.dao.handover.HandoverLineDao">
	<resultMap id="HandoverLineResultExt" type="com.baozun.scm.primservice.whoperation.model.handover.HandoverLine">
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="handoverLineColumnsExt">
	    <![CDATA[
	        	id as id,
	        	handover_id as handoverId,
	        	outboundbox_id as outboundboxId,
	        	outboundbox_code as outboundboxCode,
	        	wh_outboundbox_id as whOutboundboxId,
	        	ou_id as ouId
	    ]]>
	</sql>
	
	<sql id="handoverLineDynamicWhereExt">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用rapid中的Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>
	       
				<if test="id!=null">
					and id = #{id}
				</if>
	       
				<if test="handoverId!=null">
					and handover_id = #{handoverId}
				</if>
	       
				<if test="outboundboxId!=null">
					and outboundbox_id = #{outboundboxId}
				</if>
	       
	       
		       <if test="outboundboxCode!=null and outboundboxCode!=''">
					and outboundbox_code = #{outboundboxCode}
				</if>
	       
	       
		       <if test="whOutboundboxId!=null and whOutboundboxId!=''">
					and wh_outboundbox_id = #{whOutboundboxId}
				</if>
	       
				<if test="ouId!=null">
					and ou_id = #{ouId}
				</if>
		</where>
	</sql>
	
</mapper>
