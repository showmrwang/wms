<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">


<mapper namespace="com.baozun.scm.primservice.whoperation.dao.collect.WhOdoArchivIndexDao">
	<resultMap id="WhOdoArchivIndexResultExt" type="com.baozun.scm.primservice.whoperation.model.collect.WhOdoArchivIndex">
	</resultMap>
	
	<!-- 用于select查询公用抽取的列 -->
	<sql id="whOdoArchivIndexColumnsExt">
	    <![CDATA[
	        	id as id,
	        	ec_order_code as ecOrderCode,
	        	ou_id as ouId,
	        	sys_date as sysDate,
	        	data_source as dataSource
	    ]]>
	</sql>
	
	<sql id="whOdoArchivIndexDynamicWhereExt">
		<!-- ognl访问静态方法的表达式 为@class@method(args),以下为调用rapid中的Ognl.isNotEmpty()方法,还有其它方法如isNotBlank()可以使用，具体请查看Ognl类 -->
		<where>
	       
				<if test="id!=null">
					and id = #{id}
				</if>
	       
	       
		       <if test="ecOrderCode!=null and ecOrderCode!=''">
					and ec_order_code = #{ecOrderCode}
				</if>
	       
				<if test="ouId!=null">
					and ou_id = #{ouId}
				</if>
	       
	       
		       <if test="sysDate!=null and sysDate!=''">
					and sys_date = #{sysDate}
				</if>
	       
	       
		       <if test="dataSource!=null and dataSource!=''">
					and data_source = #{dataSource}
				</if>
		</where>
	</sql>
	
</mapper>
