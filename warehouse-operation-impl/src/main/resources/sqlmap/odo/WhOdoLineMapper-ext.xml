            <if test="id!=null">
                and id = #{id}
            </if>

            <if test="odoId!=null">
                and odo_id = #{odoId}
            </if>

            <if test="linenum!=null">
                and linenum = #{linenum}
            </if>

            <if test="store!=null and store!=''">
